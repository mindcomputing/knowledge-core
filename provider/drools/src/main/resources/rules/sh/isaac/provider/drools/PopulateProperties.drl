rule "Setup modules"
   lock-on-active true
when
   $propertySheetItem : PropertySheetItem(getSpecification().getNid() == MetaData.MODULE_NID_FOR_VERSION____SOLOR.getNid());
then
   $propertySheetItem.setEditorType(EditorType.CONCEPT_SPEC_CHOICE_BOX);
   $propertySheetItem.setDefaultValue(MetaData.SOLOR_OVERLAY_MODULE____SOLOR);
   $propertySheetItem.getAllowedValues().add(MetaData.SOLOR_OVERLAY_MODULE____SOLOR);
   $propertySheetItem.getAllowedValues().add(MetaData.US_EXTENSION_MODULES____SOLOR);
   $propertySheetItem.getAllowedValues().add(MetaData.VHAT_MODULES____SOLOR);
   $propertySheetItem.getAllowedValues().add(MetaData.CVX_MODULES____SOLOR);
   $propertySheetItem.getAllowedValues().add(MetaData.HL7_V3_MODULES____SOLOR);
   $propertySheetItem.getAllowedValues().add(MetaData.ICD10_MODULES____SOLOR);
   $propertySheetItem.getAllowedValues().add(MetaData.LOINC_MODULES____SOLOR);
   $propertySheetItem.getAllowedValues().add(MetaData.MVX_MODULES____SOLOR);
   $propertySheetItem.getAllowedValues().add(MetaData.NUCC_MODULES____SOLOR);
   $propertySheetItem.getAllowedValues().add(MetaData.RXNORM_MODULES____SOLOR);
   $propertySheetItem.getAllowedValues().add(MetaData.SNOMED_CT_CORE_MODULES____SOLOR);
end

